{
  "criterion":
  {
      "name": "MSELoss",
      "patameters":
      {
        "reduction": "mean"
      }
  },
  "optimizer":
  {
      "name": "SGD",
      "parameters":
      {
        "lr": 1e-2,
        "momentum": 0.9
      }
  },
  "scheduler":
  {
      "name": "StepLR",
      "parameters":
      {
        "step_size": 1,
        "gamma": 0.999
      },
      "scheduler_decay": 0.5,
      "scheduler_decay_period": 20
  },
  "init_epoch": 0,
  "training_epochs": 3,
  "training_monitoring_period": 1,
  "saving_period": 1,
  "dataloader_parameters":
  {
      "batch_size": 128,
      "shuffle": false,
      "num_workers": 0
  },
  "train_on_gpu": true,
  "dataset":
  {
    "name": "BasicCifar10Dataset",
    "input_conversions": [{"name":"CustomNormalize",
                        "parameters": [50, 100]}],
    "output_conversions": [{"name":"CustomNormalize",
                        "parameters": [0, 255.0]}],
    "transforms": [{"name": "ToTensor",
                    "parameters": []}],
    "blur": {
      "do_blur": false,
      "kernel_size": [5, 5]}
  }

}
